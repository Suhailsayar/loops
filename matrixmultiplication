// c program to do simple matrix multiplication
#include <stdio.h>
#define SIZE 2
void multiply(int first[SIZE][SIZE], int second[SIZE][SIZE], int result[
SIZE][SIZE]) {
    for (int i = 0; i < SIZE; i++) {
        for (int j = 0; j < SIZE; j++) {
            result[i][j] = 0;
            for (int k = 0; k < SIZE; k++) {
                result[i][j] += first[i][k] * second[k][j];
            }
        }
    }
}
int main() {
    int first[SIZE][SIZE] = { {1, 2}, {3, 4} };
    int second[SIZE][SIZE] = { {5, 6}, {7, 8} };
    int result[SIZE][SIZE];
    multiply(first, second, result);
    printf("Resultant Matrix:\n");
    for (int i = 0; i < SIZE; i++) {
        for (int j = 0; j < SIZE; j++) {
            printf("%d ", result[i][j]);
        }
        printf("\n");
    }
    return 0;
}